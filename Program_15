// Write a C++ program to calculate square of a given number using inline function.

#include<iostream>
using namespace std;
inline int square(int no1)
{
	return no1*no1;
}
int main()
{
	int no1;
	 cout<<"My Name Is Bhalani Pal"<<endl;
    cout<<"En_no :: 220130318080 "<<endl;
	cout<<"Enter the number::";
	cin>>no1;
	cout<<"Square number is::"<<square(no1);
	
	return 0;
}

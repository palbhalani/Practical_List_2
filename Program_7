// Write a C++ program that demonstrates use of enum user-defined data type.

#include <iostream>
using namespace std;

enum boolean
{
    f,t
};
enum days
{
    sun,mon,thu,wen,thr,fri,sat
};
int main()
{
   cout<<"My Name Is Bhalani Pal"<<endl;
    cout<<"En_no :: 220130318080 "<<endl;

    cout << "Enter the false or true : ";
    cout << "False" << f << endl;
    cout << "True" << t << endl;

    cout << "Sunday: " << sun << endl;
    cout << "Monday: " << mon << endl;
    cout << "Thuday: " << thu << endl;
    cout << "Wednesday: " << wen << endl;
    cout << "Thresday: " << thr << endl;
    cout << "Friday: " << fri << endl;
    cout << "Satrday: " << sat << endl;
    return 0;
}

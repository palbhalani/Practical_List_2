// Write a C++ program to demonstrate use of call by refrence.


#include<iostream>
using namespace std;
void swap(int &,int &);
int main()
{
    int a=10, b=20;

     cout<<"My Name Is Bhalani Pal"<<endl;
    cout<<"En_no :: 220130318080 "<<endl;
    cout<<"Before swapping the value is "<<a<<" & "<<b<<endl;
    swap(a,b);
    cout<<"After swapping the value is "<<a<<" & "<<b<<endl;
    return 0;
}
void swap(int &p, int &q)
{
    int temp = p;
        p = q;
        q = temp;
}
